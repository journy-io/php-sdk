{
    "name": "journy-io/sdk",
    "type": "library",
    "description": "This is the official PHP SDK for journy.io",
    "keywords": [
        "journy-io",
        "sdk"
    ],
    "homepage": "https://github.com/journy-io/sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Ott",
            "email": "hans@journy.io",
            "homepage": "https://github.com/hansott",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "suggest": {
        "kriswallsmith/buzz": "PSR-18 HTTP client",
        "nyholm/psr7": "PSR-7 and PSR-17 implementation"
    },
    "require-dev": {
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JournyIO\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JournyIO\\SDK\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
